<UserControl x:Class="MathieuShopKornilova.Components.ServiceControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:MathieuShopKornilova.Components"
             mc:Ignorable="d" 
             d:DesignHeight="200" MinHeight="200" MinWidth="400" d:DesignWidth="400">
    <Border BorderBrush="Blue" BorderThickness="1">
        <Grid>
        <TextBlock HorizontalAlignment="Center" VerticalAlignment="Center" Name="NameServiceTb" Text="{Binding ServiceName}"></TextBlock>
            
    </Grid>
    </Border>
    
</UserControl>
